{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  
    "compilerOptions": {
      "allowJs": true,
      "checkJs": true,
      "jsx": "Preserve",
      "target": "ESNext",
      "module": "ESNext",
      "moduleResolution": "Bundler",
      "allowImportingTsExtensions": true
    },
    "vueCompilerOptions": {
      "target": 3.3
    
  }
}